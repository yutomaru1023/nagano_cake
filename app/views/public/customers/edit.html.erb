<p id="notice"></p>
<h4>会員情報編集</h4>

<%= form_with model: @custoer, url public_customer_path do |f| %>

 <div class="form-group">
   <label>氏名</label>
   <%= f.text_field :last_name %> <%= f.text_field :first_name %>
 </div>

 <div class="form-group">
   <label>フリガナ</label>
   <%= f.text_field :last_name_kana %> <%= f.text_field :first_name_kana %>
 </div>

 <div class="form-group">
   <label>郵便番号</label>
   <%= f.text_field :postal_code %>
 </div>

 <div class="form-group">
   <label>住所</label>
   <%= f.text_field :address %>
 </div>

 <div class="form-group">
   <label>電話番号</label>
   <%= f.text_field :telephone_number %>
 </div>

 <div class="form-group">
   <label>メールアドレス</label>
   <%= f.text_field :email %>
 </div>

